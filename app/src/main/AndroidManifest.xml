<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.shoppinglist">

    <application
        android:allowBackup="true"
        android:icon="@drawable/baseline_local_grocery_store_black_18"
        android:label="@string/app_name"
        android:roundIcon="@drawable/baseline_local_grocery_store_24"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".Activity.ShopItemDetailsActivity"></activity>
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>